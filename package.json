	"jest": {
		"rootDir": "src",
		"testRegex": "/src/tests/.*test.js$",
		"setupFiles": [
			"<rootDir>/tests/polyfills.js",
			"<rootDir>/tests/setup.js"
		],
		"verbose": true,
		"moduleNameMapper": {
			"\\.css$": "<rootDir>/tests/CSSStub.js",
			"^components(.*)$": "<rootDir>/components$1",
			"^actions(.*)$": "<rootDir>/actions$1",
			"^api(.*)$": "<rootDir>/api$1",
			"^styles(.*)$": "<rootDir>/styles$1"
		},
